{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": 0,
        "linebreak-style": 0,
        "consistent-return": 0,
        "eqeqeq": 0,
        "array-callback-return": 0,
        "no-plusplus": 0
    }
}
